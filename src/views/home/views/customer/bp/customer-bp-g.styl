.customer-bp-g
  .el-dialog
    width inherit
    padding-left 20px
    padding-right 20px
    border-radius 5px
    min-width 612px
  .el-dialog__header
    border-bottom: 1px solid #e0e6ed;
    padding 40px 0 10px 0
    .el-dialog__title
      font-weight normal
  .el-dialog__body
    padding-top 0

  .el-dialog__footer
    text-align center
  .form-line-title
    display inline-block
    min-width 63px
    //word-spacing 10px
    //margin-right 10px
  .form-line
    margin-top 30px

  .el-input-group__append
    background transparent
    border-left none
  .el-input-group--append
    input
      border-right none

