.admin-group-box
    .editor-group-dialog
        .el-dialog--tiny
            width 520px
            border-radius 10px
        .el-dialog__body
            padding 0
            margin 0 20px
        .el-dialog__header
            height 50px
            line-height 50px
            padding 0
            margin 0 20px
            position relative
            border-bottom 1px solid #E7E7E7
            span
                font-size 18px
                color #000
                font-weight normal
            .el-dialog__headerbtn
                position absolute
                right 0
                top 0
        .el-form-item__label:before
            content "" !important
        .el-form-item__label
            text-align left
            padding-right 0
            font-weight normal
        .el-form-item
            margin 30px 50px 0
        .dialog-footer2
            margin-top 30px
            padding 20px 0
            text-align center
            border-top 1px solid #E7E7E7
            .el-button+.el-button
                margin-left 30px
.delete-group-dialog
    border-radius 10px
    position relative
    .el-message-box__header
        padding 40px 0 0 80px
    .el-message-box__content
        position static
    .el-message-box__status
        font-size 24px !important
        top 40px
        left 45px
        transform inherit
    .el-message-box__message p
        font-size 14px
        font-weight normal
    .el-message-box__close
        display none
    .el-message-box__btns
        padding 0 40px 30px 0
        .el-button
            width 65px
    .el-message-box__btns button:nth-child(2)
        margin-left 30px
    .el-message-box__message
        margin-left 60px !important
    .el-message-box__content
        padding-right 40px
